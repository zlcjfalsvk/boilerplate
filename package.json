{
  "name": "@boilerplate/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "wasm:build": "wasm-pack build libs/rs --target nodejs --out-dir ../../wasm",
    "api:build": "rm -rf dist && rm -rf wasm && npm run wasm:build && nx build api && cp wasm/rs_bg.wasm dist/apps/api",
    "api:serve": "nx run api:build && cp wasm/rs_bg.wasm dist/apps/api && node dist/apps/api/main",
    "pg:gen:client": "prisma generate --schema=./prisma/postgre/schema.prisma",
    "pg:mig:dev": "prisma migrate dev --schema=./prisma/postgre/schema.prisma"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.3.2",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.2",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/swagger": "^7.3.0",
    "@prisma/client": "^5.9.1",
    "axios": "^1.6.7",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@monodon/rust": "^1.4.0",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.2",
    "@nx/eslint": "18.0.3",
    "@nx/eslint-plugin": "18.0.3",
    "@nx/jest": "18.0.3",
    "@nx/js": "18.0.3",
    "@nx/nest": "18.0.3",
    "@nx/node": "18.0.3",
    "@nx/web": "18.0.3",
    "@nx/webpack": "18.0.3",
    "@nx/workspace": "18.0.3",
    "@swc-node/register": "~1.8.0",
    "@swc/core": "~1.4.0",
    "@swc/helpers": "~0.5.6",
    "@types/jest": "^29.5.12",
    "@types/node": "~20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "~8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nx": "18.0.3",
    "prettier": "^3.2.5",
